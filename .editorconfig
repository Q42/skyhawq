# editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 4
continuation_indent_size: 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

# Special case for package.json which is auto generated
[package.json]
indent_size = 2
